<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kingja.higo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!--嘻购详情-->
        <activity
            android:name=".activity.XigoDetailActivity"
            android:screenOrientation="portrait"/>
        <!--设置界面-->
        <activity
            android:name=".activity.SettingActivity"
            android:screenOrientation="portrait"/>
        <!--我的钱包-->
        <activity
            android:name=".activity.WalletActivity"
            android:screenOrientation="portrait"/>
        <!--我的优惠券-->
        <activity
            android:name=".activity.DiscountActivity"
            android:screenOrientation="portrait"/>
        <!--我的好友-->
        <activity
            android:name=".activity.FriendsActivity"
            android:screenOrientation="portrait"/>
        <!--我的订单-->
        <activity
            android:name=".activity.OrderActivity"
            android:screenOrientation="portrait"/>
        <!--我的消息-->
        <activity
            android:name=".activity.MsgActivity"
            android:screenOrientation="portrait"/>
        <!--个人信息-->
        <activity
            android:name=".activity.PersonalActivity"
            android:screenOrientation="portrait"/>
        <!--登录-->
        <activity
            android:name=".page.login.LoginActivity"
            android:screenOrientation="portrait"/>
        <!--注册-->
        <activity
            android:name=".activity.RegisterActivity"
            android:screenOrientation="portrait"/>
        <!--找回密码-->
        <activity
            android:name=".activity.FindPasswordActivity"
            android:screenOrientation="portrait"/>
        <!--修改密码-->
        <activity
            android:name=".activity.ModifyPasswordActivity"
            android:screenOrientation="portrait"/>
        <!--修改昵称-->
        <activity
            android:name=".activity.ModifyNicknameActivity"
            android:screenOrientation="portrait"/>
        <!--更换手机号-->
        <activity
            android:name=".activity.ModifyPhoneActivity"
            android:screenOrientation="portrait"/>
        <!--消息详情-->
        <activity
            android:name=".activity.MsgDetailActivity"
            android:screenOrientation="portrait"/>
        <!--交易明细-->
        <activity
            android:name=".activity.DealListActivity"
            android:screenOrientation="portrait"/>
        <!--通讯录-->
        <activity
            android:name=".activity.ContactsActivity"
            android:screenOrientation="portrait"/>
        <!--领券中心-->
        <activity
            android:name=".activity.DiscountCenterActivity"
            android:screenOrientation="portrait"/>
        <!--购物车-->
        <activity
            android:name=".activity.ShopCartActivity"
            android:screenOrientation="portrait"/>
        <!--收银台-->
        <activity
            android:name=".activity.PayActivity"
            android:screenOrientation="portrait"/>
        <!--嬉逛分集详情-->
        <activity
            android:name=".activity.XigoMultiDetailActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>